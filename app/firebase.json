{
  "hosting": {
    "site": "lostrpg-751c1",
    "public": "public",
    "rewrites": [
      {
        "source": "/campform/**",
        "destination": "/campform/index.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint",
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ]
  }
}
